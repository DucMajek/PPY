def zadanie1(name, birthday, email, phonenumber):
    # list
    list = [name, birthday, email, phonenumber]

    # krotki
    krotki = (name, birthday, email, phonenumber)

    # dictionary
    dictionary = {
        "Name: ": name,
        "Birthday": birthday,
        "Email": email,
        "Phone number": phonenumber
    }
    print(list)
    print(krotki)
    print(dictionary)

print("Pass the data")
print("Name: ")
name = input()

print("Birthday: ")
birthDay = input()

print("Email")
email = input()

print("Phone number")
phoneNumber = int(input())
zadanie1(name, birthDay, email, phoneNumber)
