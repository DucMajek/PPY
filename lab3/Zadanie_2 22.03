def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5)+1):
        if n % i == 0:
            return False
    return True

def primes_tuple(lst):
    primes = []
    for num in lst:
        if is_prime(num):
            primes.append(num)
    return tuple(primes)

def modulo2(list):
    modulo = []
    for num in list:
        if num % 2 == 0:
            modulo.append(num)


    return tuple(modulo)

